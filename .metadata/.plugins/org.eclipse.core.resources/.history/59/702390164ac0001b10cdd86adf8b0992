package car_dealership;

public class Vehicle {

	 String model;
	 String color;
	 double price;

	public Vehicle(String model, String color, double price) {
		super();
		this.model = model;
		this.color = color;
		this.price = price;
	}

	@Override
	public String toString() {
		return "Vehicle [model=" + model + ", color=" + color + ", price=" + price + "]";
	}
	
	

}
